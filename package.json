{
  "name": "moments",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "pnpm --filter @moments/web dev",
    "build:web": "pnpm --filter @moments/web build",
    "preview:web": "pnpm --filter @moments/web preview",
    "start:mobile": "pnpm --filter @moments/mobile exec expo start",
    "build:mobile": "pnpm --filter @moments/mobile build",
    "build:shared": "pnpm --filter @moments/shared build",
    "build:web-old": "pnpm --filter ./apps/web-old build",
    "typecheck": "tsc -b",
    "postinstall": "pnpm --filter @moments/shared build || echo 'skip shared build'"
  },
  "devDependencies": {
    "metro-runtime": "0.83.3",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@10.19.0",
  "workspacesConfig": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "expo": "^54.0.19"
  }
}
